    <body>
        <referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">Out Of Stock Noticfication</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Relieve\OutOfStock\Block\CustomerTab" name="customer_tab" template="Relieve_OutOfStock::customer_tab.phtml">
            </block>
        </referenceContainer>
    </body>
</page>